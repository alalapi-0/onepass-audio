# ===== ASR 基本参数 =====
ASR_MODEL=large-v3
ASR_LANGUAGE=zh
ASR_DEVICE=cuda
ASR_COMPUTE=float16
ASR_WORKERS=2
ASR_VAD=true
AUDIO_PATTERN=*.m4a,*.wav,*.mp3,*.flac
OVERWRITE=false

# ===== 远端目录（由 sync.env / write-sync-env 生成，以下仅作占位）=====
REMOTE_DIR=/home/ubuntu/onepass
REMOTE_AUDIO=/home/ubuntu/onepass/data/audio
REMOTE_ASR_JSON=/home/ubuntu/onepass/data/asr-json
REMOTE_LOG_DIR=/home/ubuntu/onepass/out

# ===== 运行注释 =====
RUN_MODE=prod
RUN_NOTES=prod L4 24GB float16, workers=2
